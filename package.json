{
  "name": "audrownashpodcast.com-v3",
  "version": "1.0.0",
  "packageManager": "npm@10.9.2",
  "main": "index.js",
  "scripts": {
    "build": "turbo run build",
    "check-types": "turbo run check-types",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,js,mjs}\"",
